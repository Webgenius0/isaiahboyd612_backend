						{
							"listen": "prerequest",
							"script": {
								"exec": [
									"// Generate random first and last names\r",
									"const firstNames = [\"John\", \"Jane\", \"Michael\", \"Emily\", \"David\", \"Sarah\", \"Robert\", \"Jessica\", \"Daniel\", \"Laura\"];\r",
									"const lastNames = [\"Smith\", \"Johnson\", \"Williams\", \"Brown\", \"Jones\", \"Miller\", \"Davis\", \"Garcia\", \"Rodriguez\", \"Martinez\"];\r",
									"\r",
									"const firstName = firstNames[Math.floor(Math.random() * firstNames.length)];\r",
									"const lastName = lastNames[Math.floor(Math.random() * lastNames.length)];\r",
									"\r",
									"// Generate unique username (lowercase, no spaces)\r",
									"const randomNum = Math.floor(Math.random() * 10000);\r",
									"const userName = `${firstName}${lastName}${randomNum}`.toLowerCase();\r",
									"\r",
									"// Generate email using existing pattern\r",
									"const domains = [\"example.com\", \"test.com\", \"email.com\", \"demo.com\", \"gmail.com\", \"yahoo.com\", \"hotmail.com\", \"outlook.com\"];\r",
									"const randomDomain = domains[Math.floor(Math.random() * domains.length)];\r",
									"const email = `${firstName.toLowerCase()}${randomNum}@${randomDomain}`;\r",
									"\r",
									"// Generate phone number: 10-digit numeric string\r",
									"const phoneNumber = `+8801${Math.floor(100000000 + Math.random() * 900000000)}`;\r",
									"\r",
									"// LinkedIn profile URL\r",
									"const linkedinProfileUrl = `https://www.linkedin.com/in/${userName}`;\r",
									"\r",
									"// Revenue generated year: between 2000 and current year\r",
									"const currentYear = new Date().getFullYear();\r",
									"const revenueGeneratedYear = Math.floor(2000 + Math.random() * (currentYear - 2000 + 1));\r",
									"\r",
									"// Revenue generated: decimal with two places, between 0 and 100000\r",
									"const revenueGenerated = parseFloat((Math.random() * 100000).toFixed(2));\r",
									"\r",
									"// Industry experience: float with one decimal, between 0 and 30 years\r",
									"const industryExperience = parseFloat((Math.random() * 30).toFixed(1));\r",
									"\r",
									"// Present club experience: float with one decimal, between 0 and 10 years\r",
									"const presentClubExperience = parseFloat((Math.random() * 10).toFixed(1));\r",
									"\r",
									"// Lead close ratio: decimal with two places, between 0 and 100 percent\r",
									"const leadCloseRatio = parseFloat((Math.random() * 100).toFixed(2));\r",
									"\r",
									"// Set Postman globals\r",
									"pm.globals.set(\"first_name\", firstName);\r",
									"pm.globals.set(\"last_name\", lastName);\r",
									"pm.globals.set(\"user_name\", userName);\r",
									"pm.globals.set(\"email\", email);\r",
									"pm.globals.set(\"phone_number\", phoneNumber);\r",
									"pm.globals.set(\"linkedin_profile_url\", linkedinProfileUrl);\r",
									"pm.globals.set(\"revenue_generated_year\", revenueGeneratedYear);\r",
									"pm.globals.set(\"revenue_generated\", revenueGenerated);\r",
									"pm.globals.set(\"industry_experience\", industryExperience);\r",
									"pm.globals.set(\"present_club_experience\", presentClubExperience);\r",
									"pm.globals.set(\"lead_close_ratio\", leadCloseRatio);\r",
									""
								],
								"type": "text/javascript",
								"packages": {}
							}
						}
